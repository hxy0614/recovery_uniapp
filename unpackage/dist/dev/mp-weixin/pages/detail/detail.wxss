
.container {
	padding: 20px;
}
.logo-section {
	display: flex;
	flex-direction: column;
	align-items: center;
	margin-bottom: 40px;
}
.logo {
	width: 100px;
	height: 100px;
	border-radius: 20px;
}
.title {
	margin-top: 15px;
	font-size: 18px;
	color: #333;
}
.menu-list {
	background-color: #fff;
	border-radius: 10px;
}
.menu-item {
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 15px;
	border-bottom: 1px solid #eee;
}
.menu-item:last-child {
	border-bottom: none;
}
.arrow {
	color: #999;
}
